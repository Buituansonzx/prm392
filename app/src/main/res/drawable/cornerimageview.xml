<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Define the solid background color -->
    <solid android:color="#FFFFFF" /> <!-- You can set transparent if you don't need a background -->

    <!-- Define the border color and thickness -->
    <stroke
        android:width="2dp"
        android:color="#1976D2" /> <!-- Change color as needed -->

    <!-- Define the corner radius for the rounded effect -->
    <corners android:radius="16dp" /> <!-- Adjust the radius as required -->
</shape>